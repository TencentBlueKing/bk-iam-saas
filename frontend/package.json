{
  "name": "iam-center",
  "version": "1.0.0",
  "description": "Project description",
  "author": "bkfe",
  "scripts": {
    "dev": "better-npm-run dev",
    "clean": "rimraf dist",
    "clean-dll": "rimraf static/lib*",
    "build": "better-npm-run build:ee",
    "build:analyzer": "better-npm-run build:analyzer",
    "dll": "better-npm-run dll",
    "clean:ce": "rimraf dist",
    "build:ce": "better-npm-run build:ce",
    "clean:ee": "rimraf dist",
    "build:ee": "better-npm-run build:ee",
    "clean:ieod": "rimraf dist",
    "build:ieod": "better-npm-run build:ieod",
    "server": "babel-node build/paas-server.js",
    "add-license": "babel-node build/add-license",
    "lint": "eslint ./src ./build --ext .js,.ts,.tsx,.vue",
    "lint:fix": "eslint --fix ./src ./build --ext .js,.ts,.tsx,.vue",
    "postinstall1": "cd ../ && husky install && npx husky add .husky/commit-msg 'npx --no-install commitlint --edit \"$1\"'",
    "prepare1": "cd .. && husky install frontend/.husky",
    "add-husky": "cd .. && husky install frontend/.husky && cd frontend && npx husky add .husky/commit-msg 'npx --no-install commitlint --edit \"$1\"'"
  },
  "keywords": [],
  "license": "ISC",
  "betterScripts": {
    "dll": {
      "command": "babel-node build/build-dll.js"
    },
    "dev": {
      "command": "babel-node build/check-dll.js && babel-node build/dev-server.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "npm run clean && npm run clean-dll && npm run dll && babel-node build/build.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:ieod": {
      "command": "npm run clean:ieod && npm run dll && babel-node build/build.js",
      "env": {
        "NODE_ENV": "production",
        "VERSION": "ieod"
      }
    },
    "build:ce": {
      "command": "npm run clean:ce && npm run dll && babel-node build/build.js",
      "env": {
        "NODE_ENV": "production",
        "VERSION": "ce"
      }
    },
    "build:ee": {
      "command": "npm run clean:ee && npm run dll && babel-node build/build.js",
      "env": {
        "NODE_ENV": "production",
        "VERSION": "ee"
      }
    },
    "build:analyzer": {
      "command": "npm run clean && npm run clean-dll && npm run dll && babel-node build/build.js",
      "env": {
        "NODE_ENV": "production",
        "npm_config_report": 1
      }
    }
  },
